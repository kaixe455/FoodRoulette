{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": "F:/Proyectos/FoodRoulette",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@db/*": [
        "db/*"
      ],
      "@services/*": [
        "src/services/*"
      ]
    },
    "resolve": {
      "alias": {
        "@": "path.resolve(__dirname, './src')",
      }
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}